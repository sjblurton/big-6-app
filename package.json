{
  "name": "big-6-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "check-types": "tsc",
    "format": "npx prettier . --write",
    "lint:styles": "stylelint '**/*.{css,scss}' --fix",
    "stylelint-scss-check": "stylelint-config-prettier-scss-check",
    "generate-scss-types": "npx typed-scss-modules src",
    "generate-scss-types-watch": "npx typed-scss-modules src -w",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "precommit": "npm run generate-scss-types && npm run lint && npm run check-types && npm run format && npm run lint:styles && npm run stylelint-scss-check && npm run test:coverage"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.13.4",
    "@anatine/zod-openapi": "^2.2.6",
    "@faker-js/faker": "^8.4.1",
    "@firebase/firestore": "^4.6.1",
    "@ts-rest/core": "^3.45.2",
    "@ts-rest/next": "^3.45.2",
    "@ts-rest/react-query": "^3.45.2",
    "eslint-plugin-jest": "^28.6.0",
    "firebase": "^10.11.1",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "openapi3-ts": "^4.3.3",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.5",
    "swagger-ui-react": "^5.17.14",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@typescript-eslint/utils": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-jest": "^29.1.5",
    "tsconfig-paths-jest": "^0.0.1",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.5.2"
  }
}
